{
  "mcpServers": {
    "claudine": {
      "command": "node",
      "args": ["/absolute/path/to/claudine/dist/cli.js", "mcp", "start"],
      "env": {
        "NODE_ENV": "production",
        "LOG_LEVEL": "info"
      }
    },
    "claudine-dev": {
      "command": "npx",
      "args": ["tsx", "/absolute/path/to/claudine/src/cli.ts", "mcp", "start"],
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "debug"
      }
    },
    "claudine-global": {
      "command": "claudine",
      "args": ["mcp", "start"],
      "env": {
        "LOG_LEVEL": "info"
      }
    }
  }
}